@model List<Empleado>

<h1>Detalle Factura</h1>


@{
    //List<Empleado> empleados = Context.Session.GetObject<List<Empleado>>("IDSEMPLEADOS");
    List<int> cantidad = Context.Session.GetObject<List<int>>("CANTIDAD");
}

<ul class="list-group">
    @foreach (Empleado emp in Model)
    {
        for (var i = 0; i<=Model.Count(); i++) 
        { 
            <li class="list-group-item list-group-item-danger">
                Nombre Empleado: @emp.Apellido Cantidad comprados: @cantidad[i]
            </li>
        }
    }
</ul>
