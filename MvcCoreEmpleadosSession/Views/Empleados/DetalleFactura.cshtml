@model List<int>

<h1>Detalle Factura</h1>

@{

    List<Empleado> emps = ViewData["EMPLEADOS"] as List<Empleado>;

}
<ul>
    @for (int i = 0; i < emps.Count; i++)
    {
        <li> IdEmpleado : @emps[i].IdEmpleado - Apellido: @emps[i].Apellido - Cantidad:@Model[i]</li>
        @*<li>@emps[i].IdEmpleado - @emps[i].Apellido - (@Model[i] * @emps[i].Salario)</li>*@
    }

</ul>
